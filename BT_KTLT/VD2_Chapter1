//Đệ quy nhị phân
#include<iostream>
using namespace std;
int fibonacci(int n) {
	if (n == 1 || n == 2)	return 1;
	return fibonacci(n - 1) + fibonacci(n - 2);
}
int main() {
	int n;
	cout << "Nhap n: "; cin >> n;
	cout << "So hang thu " << n << " cua day Fibonacci: " << fibonacci(n);
	return 0;
}
/*
F	=	F	=	1
 1		 2
Fibonacci: F	+ F
			n-1		n-2		,n>=3
*/
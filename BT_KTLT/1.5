	#include<iostream>
	using namespace std;
	long long f[100];
	long long sum(int n){
		long long s=0;
		if(!f[n]){	//f[n] chua duoc tinh
			for(int i=0;i<n;i++)
				s=s+sum(i)*n;
			f[n]=s;	//luu ket qua s vao f[n]     
		}
		return f[n];	//tra ve gia tri duoc luu o f[n]
	}
	int main(){
		int n;
		cout <<"Nhap n: ";cin>>n;
		f[1]=1;	
		cout <<"\nResult: "<<sum(n);
		return 0;
	} 
#include<iostream>
using namespace std;
int a[100];
int dequy(int n){
	if(n==1||n==2)	return 1;
	return dequy(n-1)+(n-1)*dequy(n-2);
}
int khu_dequy(int n){
   int s=0;
   if(!a[n]){
   	for(int i=3;i<=n;i++){
   		s=khu_dequy(n-1)+(n-1)*khu_dequy(n-2);
	   }
	a[n]=s;
   }
   return a[n];
}
int main(){
	int n;
	a[1]=1,a[2]=1;
	cout <<"Nhap n: ";	cin>>n;
	cout <<"Ket qua dung de quy: "<<dequy(n);
	cout <<"\nKet qua dung khu de quy: "<<khu_dequy(n);
	return 0;
}